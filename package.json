{
  "name": "aymusic",
  "version": "0.1.0",
  "description": "All music platform in a one platform.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "buildMac": "electron-builder -m",
    "buildWin": "electron-builder -w",
    "buildLinux": "electron-builder -l"
  },
  "author": {
    "email": "aketsuky@gmail.com",
    "url": "https://aketsuky.net",
    "name": "Shiyukine - Aketsuky™"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "https://github.com/castlabs/electron-releases#v25.1.1+wvcus",
    "electron-builder": "^24.6.3",
    "electron-forge": "^5.2.4"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.26.6",
    "cross-fetch": "^3.1.7",
    "discord-rpc": "^4.0.1",
    "electron-dl": "^3.5.0",
    "electron-is-packaged": "^1.0.2"
  },
  "build": {
    "appId": "com.aketsuky.aymusic",
    "copyright": "Aketsuky™",
    "electronDownload": {
      "mirror": "https://github.com/castlabs/electron-releases/releases/download/v"
    },
    "afterPack": "./build/afterPack.js",
    "afterSign": "./build/afterSign.js",
    "mac": {
      "category": "public.app-category.audio"
    },
    "win": {
      "verifyUpdateCodeSignature": false,
      "target": [
        "zip"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "category": "Audio",
      "target": [
        "deb"
      ],
      "icon": "build/icon.png"
    },
    "publish": {
      "provider": "github"
    }
  }
}